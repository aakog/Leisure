testSingleCondition cv dv item myloc bf carryCount counter =
  (eq cv 1)
    (eq (at item ITEM_LOCATION) CARRIED)
    (eq cv 2)
      (eq (at item ITEM_LOCATION) myloc)
      (eq cv 3)
        (or (eq (at item ITEM_LOCATION) CARRIED) (eq (at item ITEM_LOCATION) myloc))
        (eq cv 4)
          (eq myloc dv)
          (eq cv 5)
            (neq (at item ITEM_LOCATION) myloc)
            (eq cv 6)
              (neq (at item ITEM_LOCATION) CARRIED)
              (eq cv 7)
                (neq myloc dv)
                (eq cv 8)
                  (at bf dv)
                  (eq cv 9)
                    (not (at bf dv))
                    (eq cv 10)
                      (neq carryCount 0)
                      testSingleCondition2 cv dv item myloc bf carryCount counter
